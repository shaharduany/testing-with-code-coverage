{
  "include": ["src/**/*"],
  "exclude": [
    "coverage/**",
    "dist/**",
    "test/**",
    "**/*.test.js",
    "**/*.spec.js",
    "**/__tests__/**"
  ],
  "reporter": ["text", "json", "html"],
  "report-dir": "./coverage",
  "check-coverage": true,
  "branches": 80,
  "functions": 80,
  "lines": 80,
  "statements": 80
} 